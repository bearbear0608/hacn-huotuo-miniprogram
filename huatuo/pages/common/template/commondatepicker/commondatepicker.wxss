.picker-container-view{
  width: 100%;
  margin-bottom: 50rpx;
}

.picker-item-image{
  width: 40rpx;
  height: 40rpx;
}

.picker-label {
  margin: 10rpx 0;
  font-family: 'Open Sans';
  font-size: 10.5pt;
  color: #666666;
  font-weight: bold;
}

.picker-label-notes {
  font-size: 9pt;
  color: #666666;
  margin-bottom: 8rpx;

}

.picker-item-label {
  padding: 10rpx 0;
  width: 100%;
  font-family: 'Open Sans';
  color: #333333;
  font-size: 11pt;
  font-weight: normal;
}

.picker-item {
  display: flex;
  align-items:  center;
}

picker {
  border-bottom: 1rpx solid #B2B2B2;
}
